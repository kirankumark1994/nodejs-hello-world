pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/kirankumark1994/nodejs-hello-world.git'
            }
        }
        stage('Install dependencies') {
      steps {
        sh 'npm install'
      }
    }
    stage('Test') {
      steps {
         sh 'npm test'
      }
    }     
        stage('build') {
            steps {
                sh 'npm install'
            }
        }
    }
}
